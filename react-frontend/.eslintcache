ge: response.data.message,\n            successful: true\n          });\n          this.props.history.push('/getstarted');\n        },\n        error => {\n          const resMessage =\n            (error.response &&\n              error.response.data &&\n              error.response.data.message) ||\n            error.message ||\n            error.toString();\n\n          this.setState({\n            successful: false,\n            message: resMessage\n          });\n        }\n      );\n    }\n  }\n\n  render() {\n    return (\n        <div className=\"card card-container\">\n          <div className=\"center-text\">\n            <h1> SIGN UP </h1>\n          </div>\n          <Form\n            onSubmit={this.handleRegister}\n            ref={c => {\n              this.form = c;\n            }}\n          >\n            {!this.state.successful && (\n              <div>\n                <div className=\"form-group\">\n                  <label htmlFor=\"username\">Username</label>\n                  <Input\n                    type=\"text\"\n                    className=\"form-control\"\n                    name=\"username\"\n                    value={this.state.username}\n                    onChange={this.onChangeUsername}\n                    validations={[required, vusername]}\n                  />\n                </div>\n\n                <div className=\"form-group\">\n                  <label htmlFor=\"password\">Password</label>\n                  <Input\n                    type=\"password\"\n                    className=\"form-control\"\n                    name=\"password\"\n                    value={this.state.password}\n                    onChange={this.onChangePassword}\n                    validations={[required, vpassword]}\n                  />\n                </div>\n\n                <div className=\"form-group\">\n                  <button className=\"btn btn-primary btn-block\">Sign Up</button>\n                </div>\n              </div>\n            )}\n\n            {this.state.message && (\n              <div className=\"form-group\">\n                <div\n                  className={\n                    this.state.successful\n                      ? \"alert alert-success\"\n                      : \"alert alert-danger\"\n                  }\n                  role=\"alert\"\n                >\n                  {this.state.message}\n                </div>\n              </div>\n            )}\n            <CheckButton\n              style={{ display: \"none\" }}\n              ref={c => {\n                this.checkBtn = c;\n              }}\n            />\n          </Form>\n          <div className=\"center-text\">\n            Already have an account? <br />\n            Login <NavLink to='/login'> here </NavLink>\n          </div>\n        </div>\n    );\n  }\n}","/home/rohan/Desktop/Spring '21/DatabaseLab/project/Database-Project/react-frontend/src/services/user.service.js",[],"/home/rohan/Desktop/Spring '21/DatabaseLab/project/Database-Project/react-frontend/src/services/auth-header.js",[],"/home/rohan/Desktop/Spring '21/DatabaseLab/project/Database-Project/react-frontend/src/components/get-started.component.js",["71","72","73","74","75","76"],"import React, { Component } from \"react\";\nimport Form from \"react-validation/build/form\";\nimport Input from \"react-validation/build/input\";\nimport CheckButton from \"react-validation/build/button\";\n\nimport UserService from \"../services/user.service\";\n\nconst required = value => {\n    if (!value) {\n        return (\n            <div className=\"alert alert-danger\" role=\"alert\">\n                This field is required\n            </div>\n        )\n    }\n};\n\nconst vusername = value => {\n\n};\n\nconst vpassword = value => {\n\n};\n\n\nexport default class GetStarted extends Component {\n    constructor(props) {\n        super(props);\n\n        this.state = {\n            gender: 'male',\n            age: 20,\n          